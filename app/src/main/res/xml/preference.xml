<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <CheckBoxPreference
        android:defaultValue="false"
        android:icon="@drawable/twotone_nights_stay_24"
        android:key="dark"
        android:title="@string/dark" />

    <SwitchPreference
        android:defaultValue="true"
        android:icon="@drawable/twotone_insert_photo_24"
        android:key="imagequality"
        android:summaryOff="@string/low"
        android:summaryOn="@string/high"
        android:title="@string/image" />

    <Preference
        android:enabled="true"
        android:icon="@drawable/twotone_description_24"
        android:key="license"
        android:selectable="true"
        android:summary=""
        android:title="@string/license" />

    <Preference
        android:enabled="true"
        android:icon="@drawable/twotone_info_24"
        android:key="Version"
        android:selectable="true"
        android:summary="2.1.2"
        android:title="@string/version" />

    <Preference
        android:enabled="true"
        android:icon="@drawable/twotone_share_24"
        android:key="Share"
        android:selectable="true"
        android:title="@string/shareappdetails" />

    <Preference
        android:enabled="true"
        android:icon="@drawable/twotone_insert_emoticon_24"
        android:key="About"
        android:selectable="true"
        android:title="@string/about" />

</PreferenceScreen>
